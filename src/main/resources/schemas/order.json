{ "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "Order",
  "description": "A single order created from requisition",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "externalId": {
      "type": "string",
      "title": "externalId"
    },
    "emergency": {
      "type": "boolean",
      "title": "emergency"
    },
    "facilityId": {
      "type": "string",
      "title": "facilityId"
    },
    "processingPeriodId": {
      "type": "string",
      "title": "processingPeriodId"
    },
    "createdDate": {
      "type": "string",
      "title": "createdDate"
    },
    "createdById": {
      "type": "string",
      "title": "createdBy"
    },
    "programId": {
      "type": "string",
      "title": "programId"
    },
    "requestingFacilityId": {
      "type": "string",
      "title": "requestingFacilityId"
    },
    "receivingFacilityId": {
      "type": "string",
      "title": "receivingFacilityId"
    },
    "supplyingFacilityId": {
      "type": "string",
      "title": "supplyingFacilityId"
    },
    "orderCode": {
      "type": "string",
      "title": "orderCode"
    },
    "status": {
      "type": "string",
      "title": "status"
    },
    "quotedCost": {
      "type": "number",
      "title": "quotedCost"
    },
    "orderLineItems": {
      "type": "array",
      "title": "orderLineItems",
      "items": {
        "type": "object",
        "$ref": "orderLineItem.json"
      },
      "uniqueItems": false
    },
    "required": [
      "id",
      "emergency",
      "createdById",
      "programId",
      "requestingFacilityId",
      "receivingFacilityId",
      "supplyingFacilityId",
      "status",
      "quotedCost"
    ]
  }
}