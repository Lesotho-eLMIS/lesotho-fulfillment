{   "type": "object",
  "$schema": "http://json-schema.org/draft-03/schema",
  "title": "OrderFileTemplate",
  "description": "A single orderFileTemplate",
  "properties": {
    "id": {
      "type": "string",
      "required": true,
      "title": "id"
    },
    "filePrefix": {
      "type": "string",
      "required": false,
      "title": "filePrefix"
    },
    "headerInFile": {
      "type": "boolean",
      "required": false,
      "title": "headerInFile"
    },
    "orderFileColumns": {
      "type": "array",
      "required": false,
      "title": "orderFileColumns",
      "items": {
        "type": "object",
        "$ref": "orderFileColumn.json"
      },
      "uniqueItems": false
    }
  }
}