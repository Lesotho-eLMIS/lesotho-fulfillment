{   "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "CsvFileTemplateDto",
  "description": "A single csvFileTemplate",
  "properties": {
    "id": {
      "type": ["string", "null"],
      "title": "id"
    },
    "filePrefix": {
      "type": "string",
      "title": "filePrefix"
    },
    "headerInFile": {
      "type": "boolean",
      "title": "headerInFile"
    },
    "csvFileColumns": {
      "type": "array",
      "title": "csvFileColumns",
      "items": {
        "type": "object",
        "$ref": "csvFileColumnDto.json"
      },
      "uniqueItems": false
    }
  },
  "required": [
    "filePrefix",
    "headerInFile"
  ]
}
