{   "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "ProofOfDeliveryLineItem",
  "description": "A single proofOfDeliveryLineItem ",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "orderLineItem": {
      "type": "object",
      "$ref": "orderLineItem.json",
      "title": "orderLineItem"
    },
    "proofOfDelivery": {
      "type": "object",
      "$ref": "proofOfDelivery.json",
      "title": "proofOfDelivery"
    },
    "packToShip": {
      "type": "integer",
      "title": "packToShip"
    },
    "quantityShipped": {
      "type": "integer",
      "title": "quantityShipped"
    },
    "quantityReceived": {
      "type": "integer",
      "title": "quantityReceived"
    },
    "quantityReturned": {
      "type": "integer",
      "title": "quantityReturned"
    },
    "replacedProductCode": {
      "type": "string",
      "title": "replacedProductCode"
    },
    "notes": {
      "type": "string",
      "title": "notes"
    },
    "requisition": [
      "id",
      "orderLineItem",
      "proofOfDelivery"
    ]
  }
}