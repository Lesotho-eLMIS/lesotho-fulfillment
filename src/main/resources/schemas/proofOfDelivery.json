{ "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "ProofOfDelivery",
  "description": "A single proofOfDelivery of ordered products",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "order": {
      "type": "object",
      "$ref": "order.json"
    },
    "proofOfDeliveryLineItems": {
      "type": "array",
      "title": "proofOfDeliveryLineItems",
      "items": {
        "type": "object",
        "$ref": "proofOfDeliveryLineItem.json"
      },
      "uniqueItems": false
    },
    "totalShippedPacks": {
      "type": "integer",
      "title": "totalShippedPacks"
    },
    "totalReceivedPacks": {
      "type": "integer",
      "title": "totalReceivedPacks"
    },
    "totalReturnedPacks": {
      "type": "integer",
      "title": "totalReturnedPacks"
    },
    "deliveredBy": {
      "type": "string",
      "title": "deliveredBy"
    },
    "receivedBy": {
      "type": "string",
      "title": "receivedBy"
    },
    "receivedDate": {
      "type": "string",
      "title": "receivedDate"
    },
    "required": [
      "id",
      "order"
    ]
  }
}