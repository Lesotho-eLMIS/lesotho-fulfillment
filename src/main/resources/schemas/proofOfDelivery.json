{ "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "ProofOfDelivery",
  "description": "A single proofOfDelivery of ordered products",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "proofOfDeliveryLineItems": {
      "type": ["array", "null"],
      "title": "proofOfDeliveryLineItems",
      "items": {
        "type": "object",
        "$ref": "proofOfDeliveryLineItem.json"
      },
      "uniqueItems": false
    },
    "totalShippedPacks": {
      "type": "integer",
      "title": "totalShippedPacks"
    },
    "totalReceivedPacks": {
      "type": "integer",
      "title": "totalReceivedPacks"
    },
    "totalReturnedPacks": {
      "type": "integer",
      "title": "totalReturnedPacks"
    },
    "deliveredBy": {
      "type": "string",
      "title": "deliveredBy"
    },
    "receivedBy": {
      "type": "string",
      "title": "receivedBy"
    },
    "receivedDate": {
      "type": ["array", "null"],
      "title": "createdDate",
      "items": {
        "type": "number"
      },
      "uniqueItems": false
    }
  },
  "oneOf": [
    {
      "properties": {
        "order": {
          "type": ["string", "null"],
          "title": "order"
        }
      }
    },
    {
      "properties": {
        "order": {
          "type": "object",
          "$ref": "order.json",
          "title": "order"
        }
      }
    }
  ],
  "required": [
    "id",
    "order"
  ]
}
