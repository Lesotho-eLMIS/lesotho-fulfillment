{ "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "OrderLineItem",
  "description": "A single orderLineItem representing ordered product",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "orderableProductId": {
      "type": "string",
      "title": "orderableProductId"
    },
    "orderedQuantity": {
      "type": "integer",
      "title": "orderedQuantity"
    },
    "order": {
      "type": "object",
      "$ref": "order.json",
      "title": "order"
    },
    "filledQuantity": {
      "type": "integer",
      "title": "filledQuantity"
    },
    "approvedQuantity": {
      "type": "integer",
      "title": "approvedQuantity"
    },
    "required": [
      "id",
      "orderableProductId",
      "orderedQuantity",
      "filledQuantity",
      "approvedQuantity"
    ]
  }
}
