{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "OrderNumberConfiguration",
  "description": "A single orderNumberConfiguration describing how orders number will look like",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "orderNumberPrefix": {
      "type": "string",
      "title": "orderNumberPrefix"
    },
    "includeOrderNumberPrefix": {
      "type": "boolean",
      "title": "includeOrderNumberPrefix"
    },
    "includeProgramCode": {
      "type": "boolean",
      "title": "includeProgramCode"
    },
    "includeRequisitionTypeSuffix": {
      "type": "boolean",
      "title": "includeRequisitionTypeSuffix"
    },
    "required": [
      "id",
      "includeOrderNumberPrefix",
      "includeProgramCode",
      "includeRequisitionTypeSuffix"
    ]
  }
}