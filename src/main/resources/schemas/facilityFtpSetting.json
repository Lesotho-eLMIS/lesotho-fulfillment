{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "FacilityFtpSetting",
  "description": "This object contains information about ftp server related with a facility",
  "properties": {
    "id": {
      "type": [
        "string",
        "null"
      ],
      "title": "id"
    },
    "facilityId": {
      "type": "string",
      "title": "facilityId"
    },
    "protocol": {
      "type": "string",
      "title": "protocol"
    },
    "username": {
      "type": "string",
      "title": "username"
    },
    "password": {
      "type": "string",
      "title": "password"
    },
    "serverHost": {
      "type": "string",
      "title": "serverHost"
    },
    "serverPort": {
      "type": "number",
      "title": "serverPort"
    },
    "remoteDirectory": {
      "type": "string",
      "title": "remoteDirectory"
    },
    "localDirectory": {
      "type": "string",
      "title": "localDirectory"
    },
    "passiveMode": {
      "type": "boolean",
      "title": "passiveMode"
    }
  },
  "required": [
    "facilityId",
    "protocol",
    "username",
    "serverHost",
    "serverPort",
    "remoteDirectory",
    "localDirectory",
    "passiveMode"
  ]
}
